class sazid:
    def __init__(self,name1,id,name2,id2):
        self.name1 = name1
        self.id = id
        self.__name2 = name2
        self.__id2 = id2

ob = sazid("sazid",203,"arefin",204) 
print(ob.name1)
print(ob.id)
print(ob.__name2)
print(ob.__id2)